<struts>
	<!-- v1.6.7.2 RDPROJECT-820[1] zza 2014-09-23 add -->
	<package name="manageBorrowSet" namespace="/admin/auto"
		extends="wzd-admin">
		<!-- v1.6.6.2 RDPROJECT-235 zza 2013-10-21 start -->
		<!-- 自动投标用户设置列表 -->
		<action name="showAllBorrowSetAuto" class="manageBorrowSetAutoAction" method="showAllBorrowSetAuto">
			<result name="success" type="ftl">/admin/auto/showAllBorrowSetAuto.html</result>
		</action>
		
		<!-- 修改自动投标用户设置信息-->
		<action name="viewBorrowSetAuto" class="manageBorrowSetAutoAction" method="viewBorrowSetAuto">
			<result name="success" type="ftl">/admin/auto/viewBorrowSetAuto.html</result>
		</action>
		
		<!-- 删除自动投标用户设置-->
		<action name="delBorrowSetAuto" class="manageBorrowSetAutoAction" method="delBorrowSetAuto">
			<result name="success" type="ftl">/admin/auto/delBorrowSetAuto.html</result>
		</action>
		
		<!-- 自动投标记录 -->
		<action name="showAllAutoTender" class="manageBorrowSetAutoAction" method="showAllAutoTender">
			<result name="success" type="ftl">/admin/auto/showAllAutoTender.html</result>
		</action>
			<!-- 自动投标修改记录 -->
		<action name="showAllSetAutoLog" class="manageBorrowSetAutoAction" method="showAllSetAutoLog">
			<result name="success" type="ftl">/admin/auto/showAllSetAutoLog.html</result>
		</action>
	</package>
